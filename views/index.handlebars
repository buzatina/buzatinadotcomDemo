<body>
  <div class="section-index">

    <div class="container-logo w-container">
      <h1 class="h-logo">Lacat&gt;</h1>
      <h5 class="h-slogan">WE SIMULATE EXPERIENCE</h5>
    </div>

    <div class="div-tabs">
      <div class="tabs w-tabs" data-duration-in="300" data-duration-out="100">
        
        <div class="tabs-menu w-tab-menu">
          <a class="tab-link w-inline-block w-tab-link" data-w-tab="Tab 1">
            <div>Simulate With Us</div>
          </a>
          <a class="tab-link w--current w-inline-block w-tab-link" data-w-tab="Tab 2">
            <div>Experience</div>
          </a>
          <a class="tab-link w-inline-block w-tab-link" data-w-tab="Tab 3">
            <div>About Lacat&gt;</div>
          </a>
        </div>

        <div class="tabs-content w-tab-content">

          <div class="tap-pane-simulate-with-us w--tab-active w-tab-pane" data-w-tab="Tab 1">
            <div class="div-animate">
              <div class="columns w-row">
                <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4"></div>
                <div class="column-style w-col w-col-4 w-col-small-4 w-col-tiny-4">
                  <div class="div-circle-1" data-ix="new-interaction"><br>Product<br>Reputation</div>
                </div>
                <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4"></div>
              </div>
            </div>
            <div class="div-animate">
              <div class="columns w-row">
                <div class="column-style w-col w-col-4 w-col-small-4 w-col-tiny-4">
                  <div class="div-circle-4" data-ix="new-interaction-4"><br>Simulate<br>Experience</div>
                </div>
                <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4"></div>
                <div class="column-style w-col w-col-4 w-col-small-4 w-col-tiny-4">
                  <div class="div-circle-2" data-ix="new-interaction-2"><br>Heuristic<br>Evaluations</div>
                </div>
              </div>
            </div>
            <div class="div-animate">
              <div class="colums w-row">
                <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4"></div>
                <div class="column-style w-col w-col-4 w-col-small-4 w-col-tiny-4">
                  <div class="div-circle-3" data-ix="new-interaction-3"><br><br>Ethnographic<br>Studies</div>
                </div>
                <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4"></div>
              </div>
            </div>
          </div>

          <div class="tab-pane-experience w-tab-pane" data-w-tab="Tab 2">

            <div class="mask-css w-slider" data-animation="slide" data-duration="500" data-infinite="1">
              
              <div class="mask w-slider-mask">

                {{#if experiences}}

                  {{#each experiences}}

                  <div class="slide w-slide">
                    <div class="div-image">
                      <img class="img-slider-css" src="{{this.fileUrl}}">
                    </div>
                  </div>

                  {{/each}}
                  
                {{/if}}
              


              </div>

              <div class="left-arrow w-slider-arrow-left">
                <div class="w-icon-slider-left"></div>
              </div>
              <div class="right-arrow w-slider-arrow-right">
                <div class="w-icon-slider-right"></div>
              </div>
              <div class="w-round w-slider-nav"></div>
            </div>

          </div>

          <div class="w-tab-pane" data-w-tab="Tab 3">
            <div class="w-row">
              <div class="w-col w-col-2"></div>
              <div class="w-col w-col-8">
                <div class="div-about-lacat">
                  <h4 class="h4-about-lacat">Founded in 2017</h4>
                  <h4 class="h4-about-lacat">Based in South Africa</h4>
                  <h4 class="h4-about-lacat">Tel: 082 352 0497</h4>
                  <h4 class="h4-about-lacat">email: simulate@lacat.co.za</h4>
                  <h4 class="h4-about-lacat">www.lacat.co.za</h4>
                </div>
              </div>
              <div class="w-col w-col-2"></div>
            </div>
          </div>
        </div>

      </div>
      <div class="container-div w-container"><a class="button-login w-button" href="/users/login">Login</a><a class="button-login w-button" href="/users/register">Join</a></div>
    </div>
  </div>
  <div class="section-landscape"><img class="img-landscape" data-ix="new-interaction-5" sizes="100vw" src="images/cowpow-with-kids.jpeg" srcset="images/cowpow-with-kids-p-800.jpeg 800w, images/cowpow-with-kids-p-1080.jpeg 1080w, images/cowpow-with-kids.jpeg 1141w"></div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js" type="text/javascript"></script>
  <script src="https://s3-us-west-2.amazonaws.com/lacatbucket/js/webflow.js" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
</body>