<body class="events-iz section-main-home">
  <div class="container-event w-container">

      {{#if event}}

        {{#each event}}

        <img class="img-profile" sizes="(max-width: 479px) 100vw, 416.234375px" src="{{this.fileUrl}}">
        
        <div class="div-block-event-description">

          <h5 class="heading-2">{{this.eventName}}</h5>

          <p class="paragraph">{{this.eventDescription}}</p>

          <h6 class="heading-2">{{this.eventLineUp}}</h6>

          <h6 class="heading-2">{{this.eventDateAndVenue}}</h6>

          <div class="div-block-boss">
            
            <h5 class="h5-catch-google-play-attention">Book on our App</h5>

            <a href="https://play.google.com/store/apps/details?id=com.viibe&amp;hl=en">
                <img class="img-google-play" sizes="(max-width: 479px) 78vw, 193.796875px" src="https://s3-us-west-2.amazonaws.com/lacatbucket/styles/get-it-on-google-play.png">
            </a>

          </div>

        </div> 

        {{/each}}
        
      {{/if}}

  </div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js" type="text/javascript"></script>
  <script src="https://s3-us-west-2.amazonaws.com/lacatbucket/styles/webflow.js" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
</body>